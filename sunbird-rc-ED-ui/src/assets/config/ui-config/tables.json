            "prerakId": {
                "contains": "{{osid}}"
            }
          }
        },
        "title": "My AGs",
        "fields": [
          {
            "name": "AGfullName",
            "title": "Full Name",
            "keyPath": "AGfullName"
          },
          {
            "name": "dob",
            "title": "Date of Birth",
            "keyPath": "dob"
          },
          {
            "name": "category",
