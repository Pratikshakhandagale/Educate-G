            "keyPath": "category"
          },
          {
            "custom": true,
            "type": "button",
            "buttonText": "View",
            "redirectTo": "/profile/ag-detail:id"
          }
        ]
      }
    },
    {
      "ag": {
        "api": "/AGV8/search",
        "body": {
