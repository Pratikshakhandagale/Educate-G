            "title": "Contact Number",
            "keyPath": "mobile"
          },
          {
            "name": "district",
            "title": "District",
            "keyPath": "address.district"
          },
          {
            "name": "block",
            "title": "Block",
            "keyPath": "address.block"
          },
          {
            "name": "village",
            "title": "Village",
            "keyPath": "address.village"
          },
          {
            "name": "candidateStatus",
            "title": "Candidate Status",
            "keyPath": "candidateStatus"
          },
          {
            "custom": true,
            "type": "button",
            "buttonText": "View",
            "redirectTo": "/profile/admin-prerak:id"
          }
        ]
      }
    },
    {
      "ag-attestation": {
        "api": "/AGV8/search",
