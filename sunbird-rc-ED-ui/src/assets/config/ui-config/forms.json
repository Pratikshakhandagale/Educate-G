                    {
                        "definition": "PrerakV2",
                        "fields": [
                            {
                                "name": "fullName",
                                "required": true,
                                "validation": {
                                    "message": "NAME_NOT_VALID"
                                }
                            },
                            {
                                "name": "mobile",
                                "required": true,
                                "validation": {
                                    "pattern": "[1-9]{1}[0-9]{9}",
                                    "message": "MOBILE_NO_MUST_10_DIGIT"
                                }
