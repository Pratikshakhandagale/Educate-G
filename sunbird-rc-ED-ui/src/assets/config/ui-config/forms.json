                            {
                                "name": "qualification"
                            },
                            {
                                "name": "gender"
                            },
                            {
                                "name": "age"
                            },
                            {
                                "name": "address",
                                "children": {
                                    "definition": "Address",
                                    "title": false,
                                    "fields": [
                                        {
                                            "name": "district",
                                            "dependent": {
                                                "key" : "district",
                                                "title" : "District",
                                                "type": "select",
                                                "options" : "this.locationService.getDistrict()"
                                            }
                                        },
                                        {
                                            "name": "block",
                                            "dependentOn": {
                                                "key" : "block",
                                                "title" : "Block",
                                                "dependentOn" : "district",
                                                "type": "select",
                                                "options" :  []
                                            }
                                        },
                                        {
                                            "name": "village",
                                            "dependentOn": {
                                                "key" : "village",
                                                "title" : "Village",
                                                "dependentOn" : "block",
                                                "type": "select",
                                                "options" :  []
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "name": "sourcingChannel"
                            },
                            {
                                "name": "referrerIfApplicable"
                            },
                            {
                                "name": "referenceId"
                            },
                            {
                                "name": "relationshipWithTheCandidate"
                            },
                            {
                                "name": "parentOrganization"
                            },
                            {
                                "name": "typeOfPastExp"
                            },
                            {
                                "name": "durationOfPastExp"
                            }
                            
