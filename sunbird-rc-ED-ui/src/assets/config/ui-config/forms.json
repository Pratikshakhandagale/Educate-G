    {
      "prerak-signup": {
        "api": "/PrerakV2/invite",
        "type": "entity",
        "formclass": "row",
        "header": "default2",
        "title": "Create Profile",
        "isSignupForm": "true",
        "langKey": "prerakAdmin",
        "fieldsets": [
          {
            "definition": "PrerakV2",
            "fields": [
              {
                "name": "fullName",
                "required": true,
                "validation": {
                  "message": "NAME_NOT_VALID"
                }
              },
              {
                "name": "mobile",
                "required": true,
                "validation": {
                  "pattern": "[1-9]{1}[0-9]{9}",
                  "message": "MOBILE_NO_MUST_10_DIGIT"
                }
              },
              {
                "name": "qualification"
              },
              {
                "name": "gender"
              },
              {
                "name": "age"
              },
              {
                "name": "address",
                "children": {
                  "definition": "Address",
                  "title": false,
                  "fields": [
                    {
                      "name": "district",
                      "dependent": {
                        "key": "district",
                        "title": "District",
                        "type": "select",
                        "options": "this.locationService.getDistrict()"
                      }
                    }
                  ]
                }
              },
              {
                "name": "sourcingChannel"
              },
              {
                "name": "referrerIfApplicable"
              },
              {
                "name": "referenceId"
              },
              {
                "name": "relationshipWithTheCandidate"
              },
              {
                "name": "parentOrganization"
              }
            ]
          }
        ],
        "redirectTo": "/profile/prerakv2"
      }
    },
