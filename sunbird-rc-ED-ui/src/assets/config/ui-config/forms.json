    {
      "AG-add": {
        "api": "/AG",
        "type": "entity",
        "formclass": "row",
        "title": "Add new AG",
        "fieldsets": [
          {
            "definition": "AG",
            "dependencies": {
              "mobileAvailablity": {
                  "oneOf": [
                      {
                          "properties": {
                              "mobileAvailablity": {
                                  "enum": [
                                      "हाँ"
                                  ]
                              },
                              "AGWhatsappNumber": {
                                  "type": "string",
                                  "title": "कृपया किशोरी के पास उपलब्ध फ़ोन का नंबर दर्ज करें",
                                  "validation": {
                                    "pattern": "[6-9]{1}[0-9]{9}",
                                    "message": "Mobile Number must be 10 digit number."
                                }
                              }

                          }
                      },
                      {
                          "properties": {
                              "mobileAvailablity": {
                                  "enum": [
                                      "Yes"
                                  ]
                              }
                          },
                          "required": [
                              "AGWhatsappNumber"
                          ]
                      }
                  ]
              },
              "mobileAvailablity": {
                  "oneOf": [
                      {
                          "properties": {
                              "mobileAvailablity": {
                                  "enum": [
                                      "हाँ"
                                  ]
                              },
                              "AGWhatsappNumber": {
                                  "type": "string",
                                  "title": "कृपया किशोरी के पास उपलब्ध फ़ोन का नंबर दर्ज करें",
                                  "validation": {
                                    "pattern": "[6-9]{1}[0-9]{9}",
                                    "message": "Mobile Number must be 10 digit number."
                                }
                              }

                          }
                      },
                      {
                          "properties": {
                              "mobileAvailablity": {
                                  "enum": [
                                      "Yes"
                                  ]
                              }
                          },
                          "required": [
                              "AGWhatsappNumber"
                          ]
                      }
                  ]
              }
          },
            "fields": [
              {
                  "name": "prerakName",
                  "custom": true,
                  "data": {
                    "type": "api",
                    "url": "/PrerakV2",
                    "key": "fullName"
                  },
                  "element": {
                      "widget": {
                          "formlyConfig": {
                              "type": "input",
                              "templateOptions": {
                                  "type": "hidden"
                              }
                          }
                      }
                  }
              },
              {
                "name": "AGfullName",
                "required": true
              },
              {
                "name": "dob",
                "required": true,
                "type": "date",
                "validation": {
                  "future": false
                }
              },
              {
                "name": "category",
                "required": true
              },
              {
                "name": "connectVia",
                "required": true
              },
              {
                "name": "fatherFullName",
                "required": true

              },
              {
                "name": "motherFullName",
                "required": true
              },
              {
                "name": "parentsMobileNumber",
                "required": true,
                "validation": {
                  "pattern": "[6-9]{1}[0-9]{9}",
                  "message": "Mobile Number must be 10 digit number."
              }
              },

              {
                "name": "parentsWhatsappNumber",
                "required": true,
                "validation": {
                  "pattern": "[6-9]{1}[0-9]{9}",
                  "message": "Mobile Number must be 10 digit number."
              }
              },
              {
                "name": "mobileAvailablity",
                "required": true
              },

              {
                "name": "lastStandardOfEducation",
                "required": true
              },
              {
                "name": "lastStandardOfEducationYear",
                "required": true,
                "type": "date",
                "validation": {
                  "future": false
                }
              },
              {
                "name": "reasonOfLeavingEducation",
                "required": true
              },
              {
                "name": "AGDocuments",
                "required": true
              }
            ]
          }
        ],
        "redirectTo": "/profile/myags"
      }
