                      "AGWhatsappNumber": {
                        "type": "string",
                        "title": "कृपया किशोरी के पास उपलब्ध फ़ोन का नंबर दर्ज करें",
                        "validation": {
                          "pattern": "[6-9]{1}[0-9]{9}",
                          "message": "Mobile Number must be 10 digit number."
                        }
                      }
                    }
                  },
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["Yes"]
                      }
                    },
                    "required": ["AGWhatsappNumber"]
                  }
                ]
              },
              "mobileAvailablity": {
                "oneOf": [
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["हाँ"]
                      },
                      "AGWhatsappNumber": {
                        "type": "string",
                        "title": "कृपया किशोरी के पास उपलब्ध फ़ोन का नंबर दर्ज करें",
                        "validation": {
                          "pattern": "[6-9]{1}[0-9]{9}",
                          "message": "Mobile Number must be 10 digit number."
                        }
                      }
                    }
                  },
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["Yes"]
                      }
                    },
                    "required": ["AGWhatsappNumber"]
                  }
                ]
              }
            },
            "fields": [
              {
                "name": "prerakName",
                "custom": true,
                "data": {
                  "type": "api",
                  "url": "/PrerakV2",
                  "key": "fullName"
                },
                "element": {
                  "widget": {
                    "formlyConfig": {
                      "type": "input",
                      "templateOptions": {
                        "type": "hidden"
                      }
                    }
                  }
                }
              },
              {
                "name": "AGfullName",
                "required": true,
                "validation": {
                  "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                  "message": "Name is not valid."
              }
              },
              {
                "name": "dob",
                "required": true,
                "type": "date",
                "validation": {
                  "future": false,
                  "max": "2022-09-10",
                  "message": "Please enter a valid date"
                }
              },
              {
                "name": "category",
                "required": true
              },
              {
                "name": "connectVia",
                "required": true
              },
              {
                "name": "fatherFullName",
                "required": true,
                "validation": {
                  "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                  "message": "Name is not valid."
              }

              },
              {
                "name": "motherFullName",
                "required": true,
                "validation": {
                  "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                  "message": "Name is not valid."
              }
              },
              {
                "name": "parentsMobileNumber",
                "required": true,
                "validation": {
                  "pattern": "[6-9]{1}[0-9]{9}",
                  "message": "Mobile Number must be 10 digit number."
              }
              },
              {
                "name": "sameAsAbove",
                "custom": true,
                "element": {
                    "title": "Same as above?",
                    "widget": {
                        "formlyConfig": {
                            "type": "checkbox",
                            "fieldGroupClassName": "controls",
                            "className": "checkbox",
                            "templateOptions": {
                                "required": true
                            }

                        }
                    }
                }
            },
              {
                "name": "parentsWhatsappNumber",
                "required": true,
                "validation": {
                  "pattern": "[6-9]{1}[0-9]{9}",
                  "message": "Mobile Number must be 10 digit number."
                },
                "expressionProperties": {
                  "templateOptions.disabled": "model.sameAsAbove",
                  "templateOptions.required": "!model.sameAsAbove"
                }
              },
              {
                "name": "mobileAvailablity",
                "required": true
              },

              {
                "name": "lastStandardOfEducation",
                "required": true
              },
              {
                "name": "lastStandardOfEducationYear",
                "required": true,
                "type": "date",
                "validation": {
                  "future": false
                }
              },
              {
                "name": "reasonOfLeavingEducation",
                "required": true
              },
              {
                "name": "whereStudiedLast",
                "required": true
              },
              {
                "name": "registrationStatus",
                "required": true
              },
              {
                "name": "AGDocumentsV2",
                "required": true
              }

            ]
          }
        ],
        "redirectTo": "/profile/myags"
      }
    },
